
@{
    ViewBag.Title = "View";
}

    <h2> Register Details  <a href="@Url.Action("index", "Registration")"> <span class="glyphicon glyphicon-plus"></span></a></h2>


<table class=" table">

    <th> Register ID</th>
    <th> Guest Number</th>
    <th> Payment Amount </th>
    <th> Event Name </th>
    <th> Client Email </th>
    
    </tr>
    @foreach (var i in Model)
    {
        <tr>
            <td>@i.Id</td>
            <td>@i.guestNumber</td>
            <td>@i.paymentAmount</td>
            <td>@i.eventName</td>
            <td>@i.email</td>
            

        </tr>
    }

</table>

